<Law AmendmentId="502RJNJ14005006" AmendmentNum="令和二年人事院規則一四―五―六" Era="Showa" Lang="ja" LawType="Rule" Num="" Year="24">
  <LawNum>昭和二十四年人事院規則一四―五</LawNum>
  <LawBody>
    <LawTitle>人事院規則一四―五（公選による公職）</LawTitle>
    <EnactStatement>人事院は、国家公務員法に基き、公選による公職に関し次の人事院規則を制定する。</EnactStatement>
    <MainProvision>
      <Paragraph Hide="false" Num="1" OldStyle="false">
        <ParagraphNum/>
        <ParagraphSentence>
          <Sentence WritingMode="vertical">法及び規則中公選による公職とは、次に掲げるものの職とする。</Sentence>
        </ParagraphSentence>
        <Item Delete="false" Hide="false" Num="1">
          <ItemTitle>一</ItemTitle>
          <ItemSentence>
            <Sentence WritingMode="vertical">衆議院議員</Sentence>
          </ItemSentence>
        </Item>
        <Item Delete="false" Hide="false" Num="2">
          <ItemTitle>二</ItemTitle>
          <ItemSentence>
            <Sentence WritingMode="vertical">参議院議員</Sentence>
          </ItemSentence>
        </Item>
        <Item Delete="false" Hide="false" Num="3">
          <ItemTitle>三</ItemTitle>
          <ItemSentence>
            <Sentence WritingMode="vertical">地方公共団体の長</Sentence>
          </ItemSentence>
        </Item>
        <Item Delete="false" Hide="false" Num="4">
          <ItemTitle>四</ItemTitle>
          <ItemSentence>
            <Sentence WritingMode="vertical">地方公共団体の議会の議員</Sentence>
          </ItemSentence>
        </Item>
      </Paragraph>
    </MainProvision>
    <SupplProvision AmendLawNum="平成二七年一〇月二八日人事院規則一四―五―五">
      <SupplProvisionLabel>附　則</SupplProvisionLabel>
      <Paragraph Hide="false" Num="1" OldStyle="false">
        <ParagraphNum/>
        <ParagraphSentence>
          <Sentence WritingMode="vertical">この規則は、平成二十八年四月一日から施行する。</Sentence>
        </ParagraphSentence>
      </Paragraph>
    </SupplProvision>
    <SupplProvision AmendLawNum="令和二年一一月一六日人事院規則一四―五―六" Type="Amend">
      <SupplProvisionLabel>附　則</SupplProvisionLabel>
      <Paragraph Hide="false" Num="1" OldStyle="false">
        <ParagraphNum/>
        <ParagraphSentence>
          <Sentence WritingMode="vertical">この規則は、令和二年十二月一日から施行する。</Sentence>
        </ParagraphSentence>
      </Paragraph>
    </SupplProvision>
  </LawBody>
</Law>
