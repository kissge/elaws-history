                <TableStruct>
                  <Table WritingMode="vertical">
                    <TableRow>
                      <TableColumn BorderBottom="none" BorderLeft="solid" BorderRight="solid" BorderTop="solid">
                        <Sentence Num="1" WritingMode="vertical">第百十六条第一項</Sentence>
                      </TableColumn>
                      <TableColumn BorderBottom="solid" BorderLeft="solid" BorderRight="solid" BorderTop="solid">
                        <Sentence Num="1" WritingMode="vertical">地方公共団体の機関、特定地方独立行政法人又は職員団体</Sentence>
                      </TableColumn>
                      <TableColumn BorderBottom="solid" BorderLeft="solid" BorderRight="solid" BorderTop="solid">
                        <Sentence Num="1" WritingMode="vertical">博覧会協会及び国の機関</Sentence>
                      </TableColumn>
                    </TableRow>
