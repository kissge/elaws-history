          <Item Delete="false" Hide="false" Num="3">
            <ItemTitle>三</ItemTitle>
            <ItemSentence>
              <Sentence WritingMode="vertical">第一条第二項第六号から第九号までに掲げる事項</Sentence>
            </ItemSentence>
