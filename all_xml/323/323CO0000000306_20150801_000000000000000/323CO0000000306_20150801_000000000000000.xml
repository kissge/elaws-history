    </SupplProvision>
    <SupplProvision AmendLawId="327268" AmendLawNum="昭和二七年七月三一日法律第二六八号" Extract="true" Id="ddfc1c52-dc8f-46e5-bbfd-66e3164e2a27">
      <SupplProvisionLabel>附　則</SupplProvisionLabel>
