        </ParagraphSentence>
      </Paragraph>
    </SupplProvision>
    <SupplProvision AmendLawId="419039" AmendLawNum="平成一九年三月二日政令第三九号" Extract="false" Id="a221afdc-82eb-4227-8f3f-0f763298ee43" Type="Amend">
