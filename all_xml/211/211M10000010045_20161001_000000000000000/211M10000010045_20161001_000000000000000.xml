<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<Law Era="Taisho" Lang="ja" LawType="MinisterialOrdinance" Num="045" Year="11">
  <LawNum>大正十一年司法省令第四十五号</LawNum>
  <LawBody>
    <LawTitle>大正十一年司法省令第四十五号（信託表示簿及日附アル印章調製方）</LawTitle>
    <Preamble>
      <Paragraph Num="1">
        <ParagraphNum/>
        <ParagraphSentence>
          <Sentence>信託表示簿及日附アル印章調製方左ノ通相定ム</Sentence>
        </ParagraphSentence>
      </Paragraph>
    </Preamble>
    <MainProvision>
      <Article Num="1">
        <ArticleTitle>第一条</ArticleTitle>
        <Paragraph Num="1">
          <ParagraphNum/>
          <ParagraphSentence>
            <Sentence>公証人役場ニ備フヘキ信託表示簿及日附アル印章ハ左記雛形ニ依リ之ヲ調製スヘシ</Sentence>
          </ParagraphSentence>
        </Paragraph>
      </Article>
      <Article Num="2">
        <ArticleTitle>第二条</ArticleTitle>
        <Paragraph Num="1">
          <ParagraphNum/>
          <ParagraphSentence>
            <Sentence>信託表示簿ニハ公証人其ノ枚数ヲ表紙ノ裏面ニ記載シ職氏名ヲ署シ職印ヲ押捺シ毎葉ノ綴目ニ職印ヲ以テ契印ヲ為スコトヲ要ス</Sentence>
          </ParagraphSentence>
        </Paragraph>
      </Article>
    </MainProvision>
    <SupplProvision>
      <SupplProvisionLabel>附　則</SupplProvisionLabel>
      <Paragraph Num="1">
        <ParagraphNum/>
        <ParagraphSentence>
          <Sentence>本令ハ大正十二年一月一日ヨリ之ヲ施行ス</Sentence>
        </ParagraphSentence>
      </Paragraph>
    </SupplProvision>
    <SupplProvision AmendLawId="324008" AmendLawNum="昭和二四年六月一日法務府令第八号" Extract="true" Id="0aac46a8-18b7-11e8-a242-005056b30d52">
      <SupplProvisionLabel>附　則</SupplProvisionLabel>
      <Paragraph Num="1">
        <ParagraphNum>１</ParagraphNum>
        <ParagraphSentence>
          <Sentence>この府令は、公布の日から施行する。</Sentence>
        </ParagraphSentence>
      </Paragraph>
    </SupplProvision>
