<?xml version="1.0" encoding="UTF-8"?>
<Law Era="Taisho" Year="06" Num="2" LawType="MinisterialOrdinance" Lang="ja">
  <LawNum>大正六年外務省令第二号</LawNum>
  <LawBody>
    <LawTitle>大正六年外務省令第二号（外務省主管歳入証券納付ニ関スル件）</LawTitle>
    <MainProvision>
      <Paragraph Num="1">
        <ParagraphNum/>
        <ParagraphSentence>
          <Sentence>外務省主管ノ歳入ハ別段ノ規定アルモノヲ除クノ外証券ヲ以テ之ヲ納付スルコトヲ得</Sentence>
        </ParagraphSentence>
      </Paragraph>
      <Paragraph Num="2">
        <ParagraphNum/>
        <ParagraphSentence>
          <Sentence>本令ハ公布ノ日ヨリ之ヲ施行ス</Sentence>
        </ParagraphSentence>
      </Paragraph>
    </MainProvision>
  </LawBody>
</Law>
