<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<Law Era="Showa" Lang="ja" LawType="Act" Num="337" Year="27">
  <LawNum>昭和二十七年法律第三百三十七号</LawNum>
  <LawBody>
    <LawTitle>国際連合の決議に基く民生事業のため必要な物品の無償譲渡に関する法律</LawTitle>
    <MainProvision>
      <Paragraph Hide="false" Num="1" OldStyle="false">
        <ParagraphNum/>
        <ParagraphSentence>
          <Sentence WritingMode="vertical">政府は、国際連合の決議に基いて設けられた公的機関が国際連合の決議に基いて実施する民生事業のため必要な物品を、当該機関に対し無償で譲渡することができる。</Sentence>
        </ParagraphSentence>
      </Paragraph>
    </MainProvision>
    <SupplProvision Extract="false" Type="New">
      <SupplProvisionLabel>附　則</SupplProvisionLabel>
      <Paragraph Hide="false" Num="1" OldStyle="false">
        <ParagraphNum/>
        <ParagraphSentence>
          <Sentence WritingMode="vertical">この法律は、公布の日から施行する。</Sentence>
        </ParagraphSentence>
      </Paragraph>
    </SupplProvision>
  </LawBody>
</Law>
