                  <Sentence WritingMode="vertical">単体自己資本比率</Sentence>
                  <Sentence WritingMode="vertical">四パーセント以上</Sentence>
                </TableColumn>
                <TableColumn BorderBottom="solid" BorderLeft="solid" BorderRight="solid" BorderTop="solid">
                  <Sentence WritingMode="vertical"/>
                </TableColumn>
              </TableRow>
