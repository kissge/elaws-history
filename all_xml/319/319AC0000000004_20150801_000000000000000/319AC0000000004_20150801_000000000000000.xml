          </ParagraphSentence>
        </Paragraph>
      </Article>
    </SupplProvision>
    <SupplProvision AmendLawId="322242" AmendLawNum="昭和二二年一二月二七日法律第二四二号" Id="dfcc8bf8-18c7-11e8-a136-005056b30d52">
